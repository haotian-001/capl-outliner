{
  "name": "capl-outliner",
  "displayName": "CAPL Outliner",
  "description": "Enhanced outline view for CAPL (CAN Access Programming Language) with support for structures, enumerations, classes, and all event handlers",
  "version": "0.3.0",
  "publisher": "capl-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/haotian-001/capl-outliner.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:capl"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "capl",
        "aliases": [
          "CAPL",
          "capl"
        ],
        "extensions": [
          ".can"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "vsce package --no-dependencies"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.5.4"
  }
}
